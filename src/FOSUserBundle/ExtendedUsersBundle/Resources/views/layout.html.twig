<!DOCTYPE html>
<html lang="en" class="loginform">
<head>
	<meta charset="utf-8">
	<title>Login Administration Page</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="Login Administration Page">
	<meta name="author" content="iLker Ã–zcan">

	<link href="{{ asset('bundles/fosuserbundleextendedusers/css/bootstrap.min.css') }}" rel="stylesheet">
	<link href="{{ asset('bundles/fosuserbundleextendedusers/css/style.css') }}" rel="stylesheet">

	<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	<script src="{{ asset('bundles/fosuserbundleextendedusers/js/html5shiv.js') }}" type="text/javascript"></script>
	<![endif]-->

	<!-- Fav and touch icons -->
	<link rel="shortcut icon" href="{{ asset('favicon.ico') }}">

	<script type="text/javascript" src="{{ asset('bundles/fosuserbundleextendedusers/js/jquery.min.js') }}"></script>
	<script type="text/javascript" src="{{ asset('bundles/fosuserbundleextendedusers/js/bootstrap.min.js') }}"></script>
	<script type="text/javascript" src="{{ asset('bundles/fosuserbundleextendedusers/js/scripts.js') }}"></script>
</head>

<body class="loginform">
<div class="container-fluid loginform">
	<div class="row clearfix">
		<div class="col-md-4 column"></div>
		<div class="col-md-4 column formColumn">
			<div class="formArea">
				<div class="verticalCenter">

					{# if is_granted("IS_AUTHENTICATED_REMEMBERED") #}
						{#
						{{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
						<a href="{{ path('fos_user_security_logout') }}">
							{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
						</a>
						#}
					{# else #}
						{# <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a> #}
					{# endif #}

					{% if app.request.hasPreviousSession %}
						{% for type, messages in app.session.flashbag.all() %}
							{% for message in messages %}
								<p class="label-danger errorMessageArea flash-{{ type }}">
									{{ message }}
								</p>
							{% endfor %}
						{% endfor %}
					{% endif %}

					{% block fos_user_content %}
					{% endblock fos_user_content %}
				</div>
			</div>
		</div>
		<div class="col-md-4 column">
		</div>
	</div>
</div>
</body>
</html>